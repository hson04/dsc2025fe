                    {\normalfont{\textit{\href{\VAR{exp.link}}{\VAR{exp.company}}}}}
                \BLOCK{ elif exp.company and exp.company != 'None' and exp.company != 'UNKNOWN' }
                    {\normalfont{\textit{\VAR{exp.company}}}}
                \BLOCK{ else }
                    {}
                \BLOCK{ endif }
                \BLOCK{ if exp.location and exp.location != 'None' and exp.location != 'UNKNOWN' }
                    {\normalfont{\textit{\VAR{exp.location}}}}
                \BLOCK{ else }
                    {}
                \BLOCK{ endif }
                \BLOCK{ for point in exp.description }
                    \item \VAR{point}
                \BLOCK{ endfor }
            \end{rSubsection}
        \BLOCK{ endfor }
    \end{rSection}
\BLOCK{ endif }

%==== EDUCATION SECTION ====%
\BLOCK{ if education }
\begin{rSection}{Education}
    \BLOCK{ for school in education }
        \BLOCK{ if school.university }
            \textbf{\VAR{school.university}} \hfill {\BLOCK{ if school.from_date and school.to_date }\VAR{school.from_date} - \VAR{school.to_date}\BLOCK{ elif school.from_date }\VAR{school.from_date} - Present\BLOCK{ elif school.to_date }\VAR{school.to_date}\BLOCK{ endif }} \\
            \BLOCK{ if school.degree }
                {\VAR{school.degree}}
            \BLOCK{ endif }
            \BLOCK{ if school.grade }
                \hfill {(GPA: \VAR{school.grade})}
            \BLOCK{ endif } 
            \BLOCK{ if school.coursework }
                \textbf{\textit{Relevant Courses:}} \textit{\VAR{', '.join(school.coursework)}}
            \BLOCK{ endif } 
        \BLOCK{ endif } 
    \BLOCK{ endfor }
\end{rSection}
\BLOCK{ endif }

% ==== PROJECTS SECTION =====%
\BLOCK{ if projects }
    \begin{rSection}{Projects}
        \BLOCK{ for project in projects}
            \begin{rSubsection}
                \BLOCK{ if project.link}
                    {\href{\VAR{project.link}}{\VAR{project.name}}}
                \BLOCK{else}
                    {\VAR{project.name}}
                \BLOCK{ endif }
                \BLOCK{ if (project.from_date and project.from_date != 'None' and project.from_date != 'UNKNOWN') or (project.to_date and project.to_date != 'None' and project.to_date != 'UNKNOWN') }
                    {\normalfont{\BLOCK{ if project.from_date and project.from_date != 'None' and project.from_date != 'UNKNOWN' and project.to_date and project.to_date != 'None' and project.to_date != 'UNKNOWN' }\VAR{project.from_date} - \VAR{project.to_date}\BLOCK{ elif project.from_date and project.from_date != 'None' and project.from_date != 'UNKNOWN' }\VAR{project.from_date} - Present\BLOCK{ elif project.to_date and project.to_date != 'None' and project.to_date != 'UNKNOWN' }\VAR{project.to_date}\BLOCK{ endif }}}
                \BLOCK{ else }
                    {\normalfont{}}
                \BLOCK{ endif }
                {}{}
                \BLOCK{ for point in project.description}
                    \item \VAR{point}
                \BLOCK{ endfor }
            \end{rSubsection}
        \BLOCK{ endfor }
    \end{rSection}
\BLOCK{ endif }

