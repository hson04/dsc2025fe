%===== WORK EXPERIENCE SECTION =====%
\BLOCK{ if work_experience }
    \begin{rSection}{Work Experience}
        \BLOCK{ for exp in work_experience }
            \begin{rSubsection}
                {\VAR{exp.role}}
                {\BLOCK{ if exp.from_date and exp.to_date }\VAR{exp.from_date} - \VAR{exp.to_date}\BLOCK{ elif exp.from_date }\VAR{exp.from_date} - Present\BLOCK{ elif exp.to_date }\VAR{exp.to_date}\BLOCK{ endif }}
